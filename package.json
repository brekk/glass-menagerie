{
  "name": "glass-menagerie",
  "version": "1.0.0",
  "description": "Template for making new node projects",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run build",
    "test-es6": "babel-tape-runner \"tests/**/spec-*.js\"",
    "test-core": "npm run transpile && npm run test-es6",
    "test-faucet": "npm run test-core | ./node_modules/.bin/faucet",
    "test-log": "npm run test-core > test-output.log",
    "cover": "nyc --statements 100 --lines 100 --functions 100 --branches 90 -x=lib/**/*.js -x=tests/**/*.js --r=lcov --r=json --r=html --r=text --r=text-summary npm run test-faucet",
    "report": "istanbul report",
    "transpile": "babel src -d lib",
    "build": "npm run cover",
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc src/** --rule 'no-console: 2'",
    "prepublish": "npm run build"
  },
  "bin": {
    "glass": "./cli.js"
  },
  "keywords": [],
  "author": "brekk",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^3.2.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-lodash-fp": "^2.0.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "faucet": "0.0.1",
    "istanbul": "^0.4.4",
    "must": "^0.13.2",
    "nyc": "^7.1.0",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "data.task": "^3.1.0",
    "data.validation": "^1.3.1",
    "eslint-config-standard": "^5.3.5",
    "html2jade": "^0.8.4",
    "js-yaml": "^3.6.1",
    "lodash": "^4.14.1",
    "pug": "^2.0.0-beta4",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brekk/glass-menagerie.git"
  },
  "bugs": {
    "url": "https://github.com/brekk/glass-menagerie/issues"
  },
  "homepage": "https://github.com/brekk/glass-menagerie#readme"
}
